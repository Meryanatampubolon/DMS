<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <h5 class="float-left">Pengaturan Akses Pengajuan dan Disposisi</h5>
            </div>
            <div class="card-body p-0">
                <table class="table table-super-condensed table-striped">
                    <thead class="bg-primary text-white">
                        <th class="w-30">Nama Bagian</th>
                        <th class="w-30">Sub Bagian</th>
                        <th class="w-30">Keterangan</th>

                        <th class="w-10"></th>
                    </thead>
                    <tbody>
                        <% q_departemen.forEach(row => { %>
                        <tr>
                            <td><%= row.departemen %></td>
                            <td><%= row.subDepartemen %> </td>
                            <td><%= row.keterangan %> </td>
                            <td class="text-right">
                                <a href="/aksesdokumen-edit/<%= row.departemenId %> " class="btn btn-sm btn-primary"><i class="fa fa-gears"></i>Atur</a>
                                
                            </td>
                        </tr>
                        <% }) %>
                    </tbody>  
                </table>

            </div>
        </div>
    </div>
</div>