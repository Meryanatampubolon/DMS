<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <h5 class="float-left">Bagian instansi</h5>
                <a class="btn btn-danger float-right" href="/aksesdokumen"><i class="fa fa-times"></i></a>
            </div>
            <div class="card-body p-0">
                <table class="table table-super-condensed table-striped text-lg">                    
                        <tr>
                            <td class="w-10 text-bold">Nama Bagian</td>
                            <td>: <%= q_departemen.departemen %></td>
                        </tr>
                        <tr>
                            <td class="text-bold">Keterangan :</td>
                            <td><%= q_departemen.keterangan %> </td>                           
                        </tr>
                    </tbody>  
                </table>

            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-6">
        <div class="card card-primary">
            <div class="card-header ">
                <h5 class="float-left">Akses Pengajuan</h5>
                <a class="btn btn-default float-right text-dark" href=""><i class="fa fa-plus text-dark"></i> Tambah</a>
            </div>
            <div class="card-body">
                <table class="table table-condensed table-striped">
                    <thead>
                        <th>Nama bagian</th>
                        <th></th>
                    </thead>
                </table>
            </div>
            <div class="card-footer">
                <p class="text-sm">Data ini akan menjadi pilihan ketika dokumen disampaikan pada pengajuan</p>
            </div>
        </div>
    </div>
    <div class="col-lg-6">
        <div class="card card-warning">
            <div class="card-header ">
                <h5 class="float-left">Akses Disposisi</h5>
                <a class="btn btn-default float-right text-dark" href=""><i class="fa fa-plus text-dark"></i> Tambah</a>
